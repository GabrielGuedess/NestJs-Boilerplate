{{#each fields}}
{{#if this.isEnum}}
import { type {{pascalCase this.name}}Props } from 'domain/entities/{{camelCase ../name}}/{{pascalCase ../name}}';
{{/if}}
{{/each}}

export abstract class {{pascalCase name}}ResponseDTO {
  id: string;
  {{#each fields}}
    {{#unless isHideField}}
      {{#if isEnum}}
        {{snakeCase name}}: {{pascalCase name}}Props;
      {{else if (and isOptional (not hasDefault))}}
        {{snakeCase name}}?: {{type}};
      {{else}}
        {{snakeCase name}}: {{type}};
      {{/if}}
    {{/unless}}
  {{/each}}
}
