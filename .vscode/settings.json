{
  "[handlebars]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.tslint": "explicit"
  },
  "eslint.validate": ["mdx", "markdown", "javascript", "typescript"],
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.files.associations": {
    ".czrc": "commitlint",
    "apollo.ts": "apollo",
    "husky.sh": "husky",
    "mock.ts": "log",
    "mocks.ts": "log",
    "prisma.ts": "prisma",
    "prismaClient.ts": "prisma",
    "spec.ts": "test-ts",
    "spec.tsx": "test-jsx",
    "stories.tsx": "storybook",
    "test.ts": "test-ts",
    "test.tsx": "test-jsx"
  },
  "material-icon-theme.folders.associations": {
    "@shared": "shared",
    "ISSUE_TEMPLATE": "error",
    "MailProvider": "Mail",
    "application": "app",
    "args": "custom",
    "consumers": "include",
    "domain": "core",
    "dtos": "typescript",
    "entities": "class",
    "eslint-config": "tools",
    "fragments": "mjml",
    "http": "container",
    "infrastructure": "server",
    "inputs": "mock",
    "mappers": "meta",
    "migrations": "tools",
    "modules": "components",
    "permissions": "secure",
    "queries": "command",
    "repositories": "mappings",
    "schemas": "class",
    "strategies": "keys",
    "ui": "container",
    "useCases": "controller",
    "view-models": "review",
    "workflows": "ci"
  },
  "prettier.ignorePath": ".gitignore",
  "search.exclude": {
    "package-lock.json": true,
    "pnpm-lock.yaml": true,
    "yarn.lock": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
