{
  "name": "nestjs-boilerplate",
  "version": "1.5.14",
  "private": true,
  "description": "NestJs-Boilerplate",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:database-docs": "pnpm dlx @liam-hq/cli erd build --input prisma/schema.prisma --format prisma --output-dir docs/database/erd",
    "commit": "cz",
    "find-deadcode": "ts-prune -p tsconfig.json -e",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate": "pnpm plop --plopfile generators/plopfile.mjs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0 -f mo",
    "lint:fix": "pnpm eslint . --fix --ignore-pattern '**/generated/**/**.ts'",
    "lint:interactive": "eslint-nibble \"{src,apps,libs,test}/**/*.ts\"",
    "prepare": "husky",
    "release": "semantic-release",
    "sort:package": "sort-package-json",
    "start": "nest start",
    "start:database-docs": "pnpm dlx http-server docs/database/erd",
    "start:debug": "nest start --debug -w",
    "start:dev": "nest start -w",
    "start:prod": "node dist/main",
    "test": "jest --config jest.config.ts",
    "test:ci": "jest --config jest.config.ts --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config.ts",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@aws-sdk/client-s3": "^3.803.0",
    "@devoxa/prisma-relay-cursor-connection": "^3.1.2",
    "@keyv/redis": "^4.4.0",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.0",
    "@nestjs/core": "^11.1.0",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.7.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.2",
    "cacheable": "^1.8.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.6.1",
    "colors": "^1.4.0",
    "cpf-cnpj-validator": "^1.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-relay": "^0.10.2",
    "graphql-scalars": "^1.24.2",
    "graphql-upload-ts": "^2.1.2",
    "nestjs-graphql-relay": "^10.6.0",
    "nestjs-i18n": "^10.5.1",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^10.4.0",
    "prisma-extension-pagination": "^0.7.5",
    "prisma-nestjs-graphql": "^21.1.1",
    "reflect-metadata": "^0.2.2",
